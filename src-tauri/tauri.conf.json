{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "靓仔的app",
  "version": "1.0.0",
  "identifier": "com.renteng.sales",
  "build": {
    "beforeDevCommand": "npm run dev -- --host --port 3000",
    "devUrl": "http://localhost:3000",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../build"
  },
  "app": {
    "windows": [
      {
        "title": "靓仔的app",
        "width": 1200,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "alwaysOnTop": false,
        "center": true
      }
    ],
    "security": {
      "csp": null
    },
    "withGlobalTauri": true
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "publisher": "佛山市仁腾装饰材料有限公司",
    "category": "Business",
    "shortDescription": "装饰材料销售管理系统",
    "longDescription": "专业的装饰材料销售管理系统，支持客户管理、产品管理、销售单和送货单生成等功能。",
    "copyright": "Copyright © 2024 佛山市仁腾装饰材料有限公司",
    "externalBin": [],
    "resources": [],
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "10.13",
      "exceptionDomain": ""
    },
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    }
  }
}
